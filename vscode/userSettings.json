{
    "workbench.welcome.enabled": false,
    "terminal.integrated.shell.windows": "C:\\Windows\\sysnative\\bash.exe",
    "window.zoomLevel": 0,
    "editor.cursorStyle": "block",
    // "editor.fontFamily": "ProFont for Powerline",
    // "editor.fontFamily": "Meslo LG S for Powerline",
    // "editor.fontFamily": "Linux Biolinum G",
    "editor.fontFamily": "Inconsolata for Powerline BoldForPowerline",
    // "editor.fontFamily": "Portico Rounded",
    "editor.tabCompletion": true,
    "editor.cursorBlinking": "expand",
    "editor.cursorStyle": "block",
    "folder-indexing.exclude": [
        "*\\bin\\*",
        "*\\obj\\*",
        "*\\.svn\\*",
        "*\\.git\\*",
        "*.dll",
        "*.exe",
        "*.spec.js",
        "workspace.xml"
    ],
    "folder-indexing.searchResultMaxSize": 1000,
    "search.useIgnoreFilesByDefault": true,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "*.xml": true,
        "*.spec.js":true,
        "*.min.js": true,
        "*.min.*": true,
        "*.html": true,
        ".gitignore": true,
    },
    "extensions.ignoreRecommendations": true,
    "workbench.colorCustomizations": {
        "activityBarBadge.background": "#E57373",
        "list.activeSelectionForeground": "#E57373",
        "list.inactiveSelectionForeground": "#E57373",
        "list.highlightForeground": "#E57373",
        "scrollbarSlider.activeBackground": "#E5737350",
        "editorSuggestWidget.highlightForeground": "#E57373",
        "textLink.foreground": "#E57373",
        "progressBar.background": "#E57373",
        "pickerGroup.foreground": "#E57373"
    },
    "materialTheme.cache.workbench.settings": {
        "accent": "Red",
        "accentPrevious": "Graphite",
        "themeColours": "Default"
    },
    "workbench.iconTheme": "eq-material-theme-icons",
    "workbench.colorTheme": "Material Theme"
}